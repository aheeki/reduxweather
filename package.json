{
  "name": "redux-simple-starter",
  "version": "1.0.0",
  "description": "Simple starter package for Redux with React and Babel support",
  "main": "webpack.config.js",
  "repository": "git@github.com:StephenGrider/ReduxSimpleStarter.git",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "dev": "webpack-dev-server --config webpack.config.dev.js --watch --progress --colors --hot --inline -d --host 0.0.0.0 --port 80",
    "local": "webpack-dev-server --watch --progress --colors --hot --inline --no-info -d",
    "prodbuild": "webpack --progress --display-modules --colors -p",
    "deploy": "webpack --progress --display-modules --colors -p && surge -p ./public"    
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "less-loader": "^2.2.2",
    "less": "^2.5.3",
    "redux-devtools": "^3.1.1",
    "redux-devtools-diff-monitor": "^4.0.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",    
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "babel-preset-stage-1": "^6.1.18",
    "lodash": "^3.10.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-google-maps": "^4.7.1",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.1",
    "react-sparklines": "^1.5.1",
    "redux": "^3.0.4",
    "redux-promise": "^0.5.3",
    "history": "^1.17.0",
    "react-router-redux": "^4.0.0"
  }
}
